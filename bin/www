#!/usr/bin/env node

const app = require('../app.js');

const port = process.env.CORS_DB_PORT;
const host = process.env.CORS_HOST;

app.listen(port, host, (err) => {
    if(err){
        console.log(`Could not listen to ${host}:${port}! :(`);
    } else {
        console.log(`Listening to ${host}:${port}! :)`);
    }
});
